
	<web-app id="WebApp_ID">
		<display-name>WebApp</display-name>
		<servlet>
			<servlet-name>LoginServletController</servlet-name>
			<display-name>LoginServletController</display-name>
			<description></description>
			<servlet-class>com.training.servlet.LoginServletController</servlet-class>
		</servlet>
		<servlet>
			<servlet-name>ProductDetailsController</servlet-name>
			<display-name>ProductDetailsController</display-name>
			<description></description>
			<servlet-class>com.training.servlet.ProductDetailsController</servlet-class>
		</servlet>
		<servlet>
			<servlet-name>DeleteServlet</servlet-name>
			<display-name>DeleteServlet</display-name>
			<description></description>
			<servlet-class>com.training.servlet.DeleteServlet</servlet-class>
		</servlet>
		<servlet>
			<servlet-name>UpdateProductDetails</servlet-name>
			<display-name>UpdateProductDetails</display-name>
			<description></description>
			<servlet-class>com.training.servlet.UpdateProductDetails</servlet-class>
		</servlet>
		<servlet>
			<servlet-name>logout</servlet-name>
			<display-name>logout</display-name>
